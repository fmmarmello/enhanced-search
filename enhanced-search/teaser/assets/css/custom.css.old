/* reset */
*{
	margin: 0;
	padding: 0; 
	border: 0;
}


/* fonts */
@font-face {
	font-family: 'Myriad-Pro';
	src: url('../../assets/fonts/MyriadPro-Regular.eot'); /* IE */  
	src: local('Myriad-Pro'), url('../../assets/fonts/MyriadPro-Regular.ttf') format('truetype'); /* non-IE */
	/*src: local('Myriad-Pro'), url('../../assets/fonts/MyriadPro-Regular.woff') format('woff'); /* Modern Browsers */
}

@font-face {
	font-family: 'Myriad-Pro-Bold';
	src: url('../../assets/fonts/MyriadPro-Bold.eot'); /* IE */  
	src: local('Myriad-Pro-Bold'), url('../../assets/fonts/MyriadPro-Bold.ttf') format('truetype'); /* non-IE */
	/*src: local('Myriad-Pro-Bold'), url('../../assets/fonts/MyriadPro-Bold.woff') format('woff'); /* Modern Browsers */
}

body > img {
	float: left;
}

.wrapper{
	width: 1000px;
	margin: 0 auto;
}

.pull-left{
	float:left;
}

.pull-right{
	float:right;
}


.header{
	background: url('../../assets/img/bg.jpg') no-repeat;
	/*height: 640px;*/
	height: 387px;
}
.header .wrapper{
	height: 10px;
}
.header .logo{
	background: url('../../assets/img/logo_family_v2.png') no-repeat center center;
	height: 510px;
	position: relative;
	/*top: 46px;*/
}
.header .placemark{
/*	background: url('../../assets/img/placemark.png') no-repeat;
	height: 510px;
	width: 462px;
	position: relative;
	top: -434px;	
	margin: 0 auto;
*/	
	background: url('../../assets/img/placemark_v2.png') no-repeat;
	height: 510px;
	width: 393px;
	position: relative;
	top: -532px;
	margin: 0 auto;
	right: -68px;
	overflow: hidden;
}

.header .placemark .flip-clock-wrapper {
	text-align: center;
	position: relative;
	width: 100%;
	margin: 1em;
	top: 82px;
	left: 95px;
}
.header .placemark .flip-clock-wrapper span.days{
	width: 0;
}
.header .placemark .flip-clock-wrapper ul {
	position: relative;
	float: left;
	/*margin: 5px;*/
	/*width: 48px;*/
	margin: 0px;
	width: 22px;
	height: 38px;
	border-radius: 6px;
	background: #000;
}
.header .placemark .flip-clock-wrapper ul li {
	line-height: 48px;
}
.header .placemark .flip-clock-wrapper ul li a div div.inn {
	position: absolute;
	left: 0;
	z-index: 1;
	width: 100%;
	height: 200%;
	color: #4b1703;
	text-shadow: 0 1px 2px #000;
	text-align: center;
	border-radius: 6px;
	font-size: 26px;
	line-height: 38px;
	font-weight: bold;
}
.header .placemark .flip-clock-wrapper ul li a div.up div.inn {
	/*background-color: #ff6503;*/
	background: url('../../assets/img/bg_flip.png') -12px 0;
}
.header .placemark .flip-clock-wrapper ul li a div.down div.inn {
	/*background-color: #ff6503;*/
	background: url('../../assets/img/bg_flip.png') -12px -6px;
}
.header .placemark .flip-clock-label {
	display: none;
}
.header .placemark .flip-clock-divider {
	float: left;
	display: inline-block;
	position: relative;
	width: 8px;
	height: 50px;
}
.header .placemark .flip-clock-dot {
	display: block;
	background: #888;
	width: 2px;
	height: 2px;
	position: absolute;
	border-radius: 50%;
	box-shadow: 0 0 5px rgba(0, 0, 0, 0.5);
	left: 3px;
}

/**/

.body{
	position: relative;
}
.body h1{
	font-family: 'Myriad-Pro-Bold', Verdana, Arial, Sans-serif;
	color: #20201f;
	font-size: 23px;
	letter-spacing: -1px;
	margin: -18px 0 48px 0;
	padding: 0px 0 0 58px;
	width: 480px;
}
.body .destaque{
	color: #f16e2a;
}
.body .form{
	width: 33%;
	float: left;
}
.body .form .icon{
	width: 70px;
	height: 70px;
	display: block;
	float: left;
	margin-left: 12px;
}
.body .form_contato{
	position: relative;
}
.body .form_comprar {
	border-right:  1px dashed #333;
}
.body .form_comprar .icon{
	background: url('../../assets/img/form_icons.png') no-repeat 0 0;
}
.body .form_vender {
	border-right:  1px dashed #333;
}
.body .form_vender .icon{
	background: url('../../assets/img/form_icons.png') no-repeat -80px 0;
}
.body .form_trabalhe_conosco .icon{
	background: url('../../assets/img/form_icons.png') no-repeat -160px 0;
}
.body .form h2{
	font-family: 'Myriad-Pro', Verdana, Arial, Sans-serif;
	color: #20201f;
	font-size: 19px;
	letter-spacing: 0px;
	margin: 4px 22px; 
	display: table-cell;
	vertical-align: middle;
	padding: 16px 8px;

}
.body .form input{
	font-family: 'Myriad-Pro', Verdana, Arial, Sans-serif;
	color: #666;
}
.body .form input[type="text"],
.body .form input[type="file"]{
	border-radius: 16px;
	background: #e8e2cf;
	margin: 4px 22px; 
	padding: 8px 16px;
	width: 79%;
	color: #e8e2cf;
}
.body .form label[for="curriculo"]{
	font-family: 'Myriad-Pro', Verdana, Arial, Sans-serif;
	font-size: 13px;
	padding: 4px 8px;
	position: relative;
	top: -32px;
	left: 172px;
	background: #e8e2cf;
}
.body .form select{
	border-radius: 16px;
	background: #e8e2cf;
	margin: 4px 22px; 
	padding: 8px 16px;
	width: 88%;
}
.body .form button{
	text-transform: uppercase;
	font-family: 'Myriad-Pro-Bold', Verdana, Arial, Sans-serif;
	font-weight: bold;
	border-radius: 16px;
	background-color: #f16e2a;
	padding: 6px 48px 2px; 
	color: #fff;
	font-size: 18px;
	float: right;
	margin: 16px 25px;
}

