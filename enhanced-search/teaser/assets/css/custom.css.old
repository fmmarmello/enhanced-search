.footer{
	margin-top: 128px;
	background: url('../../assets/img/bg.jpg') no-repeat;
	display: block;
	height: 64px;
	clear: both;
	color: #fff;
	font-family: 'Myriad-Pro', Verdana, Arial, Sans-serif;
}

.footer .desenvolvedor{
	padding-top: 28px;
	font-size: 12px;
}
.footer .brand{
	padding-top: 28px;
}
.footer a{
	color: #fff;
	text-decoration: none;
}
.footer a:hover{
	color: #f16e2a;
}
